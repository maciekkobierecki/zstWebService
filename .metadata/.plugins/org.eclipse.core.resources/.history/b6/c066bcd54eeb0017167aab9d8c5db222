package tests;

import java.io.IOException;

import org.junit.Test;
import org.snmp4j.PDU;

import rest.SNMPClient;

public class SNMPClientGetNextTest {
	
	@Test
	public void testgetNextTest(){
		SNMPClient client=new SNMPClient();
		PDU pdu=null;
		try {
			pdu=client.sendGetNext(".1.3.6.1.2.1.1.2.0");
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		String str=client.convertVariableBindingsToJSON(pdu);
		System.out.println(str);
	}
}
